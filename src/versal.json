    { "path": "/welcomeconfirmation", "component": "WelcomeConfirmation" },
    { "path": "/media-dashboard", "component": "MediaDashboard" },
    { "path": "/media-subcription", "component": "MediaSubcription" },
    { "path": "/manage-payments", "component": "ManagePayments" },
    { "path": "/upload-photos", "component": "UploadPhotos" },
    { "path": "/event-photos", "component": "EventPhotos" },
    { "path": "*", "component": "NotFound" }
  ]
}
